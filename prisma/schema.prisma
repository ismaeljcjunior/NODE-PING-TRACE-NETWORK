generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model host {
  ID_HOST          Int       @id @unique @default(autoincrement())
  CONTA            String
  NOME             String?   @db.VarChar(150)
  ID_EMPRESA       String?   @db.VarChar(150)
  IP_GATEWAY_HOST  String?   @db.VarChar(150)
  DESCRICAO_HOST   String?   @db.VarChar(150)
  ZONA_HOST        String?   @db.VarChar(150)
  PARTICAO_HOST    String?   @db.VarChar(150)
  QUANTIDADE_PERCA String?   @default("10")
  STATUS_HOST      String?   @db.VarChar(150)
  DT_EXECUCAO      DateTime? @default(now())
  DT_CRIACAO       DateTime? @default(now())
}

model event_host {
  ID_EVENT               Int      @id @unique @default(autoincrement())
  STATUS_ENVIO_EVETO     String   @default("PENDENTE")
  CONTA_EVENTO           String?
  DESCRICAO_HOST         String?  @db.VarChar(150)
  NOME_EVENTO            String?  @db.VarChar(150)
  ID_EMPRESA__EVENTO     String?  @db.VarChar(150)
  IP_GATEWAY_HOST_EVENTO String?  @db.VarChar(150)
  ZONA_HOST_EVENTO       String?  @db.VarChar(150)
  PARTICAO_HOST_EVENTO   String?  @db.VarChar(150)
  DT_CRIACAO             DateTime @default(now())
}
